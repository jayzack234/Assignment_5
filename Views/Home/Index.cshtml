@{
    ViewData["Title"] = "Home Page";
}
@model ProjectListViewModel 
<!--This will be our "home" page-->
@foreach (var x in Model.Projects)
{
    <partial name="ProjectSummary" model="x" />
}
<!--Use the tag helper here, and use the page model to execute the code in Home Controller
    This will be for the navigation numbers at the bottom-->
<div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true" page-class="btn"
     page-class-normal="btn-outline-dark"
     page-class-selected="btn-info" page-url-category="@Model.CurrentCategory"
     class="class="col-xs-1" align="center"
     ></div>
